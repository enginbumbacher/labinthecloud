{
  "name": "EuglenaModel",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "modelType": {
      "type": "string",
      "required": true
    },
    "configuration": {
      "type": "object",
      "required": true
    },
    "blocklyXml":{
      "type": "text",
      "required": false
    },
    "jsCode": {
      "type": "text",
      "required": false
    },
    "sensorConfigJSON": {
      "type": "text",
      "required": false
    },
    "date_created": {
      "type": "date",
      "required": true,
      "default": "$now"
    },
    "lab": {
      "type": "string",
      "required": false
    },
    "simulated": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "student": {
      "type": "belongsTo",
      "model": "Student",
      "foreignKey": ""
    },
    "results": {
      "type": "hasMany",
      "model": "Result",
      "foreignKey": "euglenaModelId"
    }
  },
  "acls": [],
  "methods": {}
}

{
  "name": "Experiment",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "configuration": {
      "type": [
        "object"
      ],
      "required": true
    },
    "expForm": {
      "type": "object",
      "required": false
    },
    "date_created": {
      "type": "date",
      "required": true,
      "default": "$now"
    },
    "demo": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "lab": {
      "type": "string",
      "required": false
    },
    "copyOfID": {
      "type": "number",
      "required": false,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "student": {
      "type": "belongsTo",
      "model": "Student",
      "foreignKey": ""
    },
    "results": {
      "type": "hasMany",
      "model": "Result",
      "foreignKey": "experimentId"
    }
  },
  "acls": [],
  "methods": {}
}

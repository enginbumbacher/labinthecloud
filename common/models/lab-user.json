{
  "name": "LabUser",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "restrictResetPasswordTokenScope": true,
  "properties": {
    "domain": {
      "type": "string"
    },
    "uuid": {
      "type": "string",
      "defaultFn": "uuidv4"
    }
  },
  "validations": [],
  "relations": {
    "labs": {
      "type": "hasMany",
      "model": "Lab",
      "foreignKey": "labUserId"
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "permission": "ALLOW",
      "principleId": "$everyone"
    }
  ],
  "methods": {}
}

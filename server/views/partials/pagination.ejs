<div class="row">
  <div class="col">
    <nav aria-label="<%= label %>">
      <ul class="pagination justify-content-center">
        <li class="page-item<%= previous == null ? " disabled" : "" %>"><a class="page-link" href="<%= previous || "#" %>">Previous</a></li>
<% links.forEach((link) => { %>
        <li class="page-item<%= link.active ? " active" : "" %>"><a class="page-link" href="<%= link.url %>"><%= link.label %></a></li>
<% }) %>
        <li class="page-item<%= next == null ? " disabled" : "" %>"><a class="page-link" href="<%= next || "#" %>">Next</a></li>
      </ul>
    </nav>
  </div>
</div>